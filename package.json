{
  "version": "0.0.1",
  "description": "Quickly setup a Next.js project with TypeScript, ESLint & Prettier",
  "homepage": "https://nazmulhaque.netlify.app/quick-eslint-prettier-setup",
  "author": {
    "name": "Nazmul Haque",
    "email": "nazmul2018s@gmail.com",
    "url": "https://nazmulhaque.netlify.app"
  },
  "keywords": [
    "react.js",
    "next.js",
    "typescript",
    "typescript",
    "eslint",
    "prettier"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint",
      "pre-push": "tsdx build && size-limit"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "quick-eslint-prettier-setup",
  "module": "dist/qu.esm.js",
  "dependencies": {
    "chalk": "5.3.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "8.0.1",
    "sade": "1.8.1",
    "semver": "^7.5.4",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/inquirer": "^9.0.7",
    "@types/ora": "^3.2.0",
    "@types/semver": "^7.5.6",
    "@types/shelljs": "^0.8.15",
    "husky": "^8.0.3",
    "size-limit": "^11.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "bin": {
    "react-project-starter-ssl": "./src/index.js"
  }
}
